<div class="container">
  <div class="row">
    <div class="col">
      <p>登録された子たち</p>
      <% @dogs.each do |dog| %>
        <%= link_to foster_dog_path(dog.id) do %>
          <%= attachment_image_tag dog, :image, :fill, 150, 150, format: 'jpeg' %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>