<div class="mt-3 col-7 align-items-center">
  <h6 class="d-inline-block mb-0">検索 <i class="fas fa-search"></i></h6>
    <div class="d-inline-block ml-3">
      <%= form_tag(search_foster_dogs_path,:method => 'get') do %>
        <%= label_tag '犬種で検索' %>
        <%= text_field_tag :search %>
        <%= submit_tag 'Search', :dog_breed => nil, class: "btn btn-outline-dark" %>
      <% end %>
    </div>
</div>