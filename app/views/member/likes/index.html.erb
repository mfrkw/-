<div class="container">
  <div class="row">
      <% @likes.each do |like| %>
        <div class="col-md-3">
          <%= link_to member_dog_path(like.dog.id) do %>
          <%= attachment_image_tag like.dog, :image, :fill, 150, 150, format: 'jpeg' %>
          <% end %>
        </div>
      <% end %>
  </div>
</div>
<%= paginate @likes %>
