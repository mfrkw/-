<div class="container">
  <div class="row">
    （全<%= @dogs.count %>頭）
      <% @dogs.each do |dog| %>
        <div class="col-md-6">
          <%= link_to member_dog_path(dog.id) do %>
          <%= attachment_image_tag dog, :image, :fill, 150, 150, format: 'jpeg'%>
          <% end %>
          <%= dog.name %>
          犬種: <%= dog.dog_breed %>
          <div id="like_<%= dog.id %>">
          <%= render "member/likes/like", dog: dog %>
          </div> 
        </div>  
      <% end %>
  </div>
</div>