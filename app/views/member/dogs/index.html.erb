
<% @dogs.each do |dog| %>
  <%= link_to member_dog_path(dog.id) do %>
    <%= attachment_image_tag dog, :image, :fill, 100, 100, format: 'jpeg' %>
  <% end %>
  <%= dog.name %>
  <%= dog.dog_breed %>
  
  <% if current_member.already_liked?(dog) %>
    <%= link_to 'いいね外す',  member_dog_likes_path(dog), method: :delete %>
  <% else %>
    <%= link_to 'いいね',  member_dog_likes_path(dog), method: :post %>
  <% end %>
<% end %>

