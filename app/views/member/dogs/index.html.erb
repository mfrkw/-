<% @dogs.each do |dog| %>
  <%= link_to member_dog_path(dog.id) do %>
    <%= attachment_image_tag dog, :image, :fill, 100, 100, format: 'jpeg' %>
  <% end %>
  <%= dog.name %>
  <%= dog.dog_breed %>

  <% if current_member.already_liked?(dog) %>
    <%= link_to member_dog_likes_path(dog), method: :delete do %>
      <i class="fas fa-heart"></i>
    <% end %>
  <% else %>
    <%= link_to member_dog_likes_path(dog), method: :post do %>
      <i class="far fa-heart"></i>
    <% end %>
  <% end %>
<% end %>

