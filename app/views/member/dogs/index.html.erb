<%= render "member/dogs/search" %>
<div class="container">
  <div class="row">
    <div class="col-md">
      <% @dogs.each do |dog| %>
        <%= link_to member_dog_path(dog.id) do %>
          <%= attachment_image_tag dog, :image, :fill, 150, 150, format: 'jpeg'%>
        <% end %>
        <div class="row">
          <div class="col">
           <div id="like_<%= dog.id %>">
        <%= render "member/likes/like", dog: dog %>
        </div> <%= dog.name %>
          </div>
        </div>
        <div class="row">
          <div class="col">
            犬種: <%= dog.dog_breed %>
          </div>
        </div>
        
      <% end %>
    </div>
  </div>
</div>