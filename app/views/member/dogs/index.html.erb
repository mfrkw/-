<%= render "member/dogs/search" %>
<div class="container">
  <div class="row text-center">
      <% @dogs.each do |dog| %>
        <div class="image_mouseover col-md-3">
          <%= link_to member_dog_path(dog.id) do %>
          <%= attachment_image_tag dog, :image, :fill, 150, 150, format: 'jpeg'%>
          <% end %>
          <div id="like_<%= dog.id %>">
            <%= render "member/likes/like", dog: dog %>
          </div>
          <p><%= dog.name %></p>
          <p>犬種: <%= dog.dog_breed %></p>
        </div>
      <% end %>
  </div>
</div>
<%= paginate @dogs %>